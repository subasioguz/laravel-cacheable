{
    "name": "fowitech/laravel-cacheable",
    "description": "This is my package laravel-cacheable",
    "keywords": [
        "Fowitech",
        "laravel",
        "laravel-cacheable"
    ],
    "homepage": "https://github.com/fowitech/laravel-cacheable",
    "license": "MIT",
    "authors": [
        {
            "name": "Oğuzhan Subaşı",
            "email": "subasioguz@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Fowitech\\Cacheable\\": "src/",
            "Fowitech\\Cacheable\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fowitech\\Cacheable\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fowitech\\Cacheable\\CacheableServiceProvider"
            ],
            "aliases": {
                "Cacheable": "Fowitech\\Cacheable\\Facades\\Cacheable"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
